{
    "fields": {
        "name": {
            "type": "varchar",
            "maxLength": 100,
            "required": true
        },
        "layout": {
            "type": "jsonArray",
            "view": "views/settings/fields/dashboard-layout",
            "inlineEditDisabled": true,
            "required": true
        },
        "dashletsOptions": {
            "type": "jsonObject",
            "disabled": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        }
    },
    "collection": {
        "orderBy": "name",
        "order": "asc"
    }
}
